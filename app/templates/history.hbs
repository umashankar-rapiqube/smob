<div id="historyheader">
    <div id="cont-lock" style="text-align:center ;color:white"><h1><b>Transaction History</b></h1></div>
</div> 
<div class="historydispaly">
  <div class="historydispaly-left">
    <p style="font-size: 20px;color: coral;text-align:center">Transaction Id :{{Transactionid}} </p>
    

<ul style="list-style-type:none">

   {{#each Transactionlist as |item|}} 
    <!--<div class="inner-historydisplay">-->
    <b>Date:</b> <li>{{item.date}}</li> 
     <b>Update By:</b>  <li>{{item.updatedBy}}</li>
     <b>Quantity :</b>   <li>{{item.Quantity}}</li>
      <b>intended-to:</b> <li>{{item.intended-to}}</li>
      <b>cost:</b> <li>{{item.cost}}</li>
      <b>Attachment:</b> <li>{{item.Attachment}}</li>
      <hr class="style3">
     <!--</div>-->
     <br/>
  {{/each}}
  </ul>
 </div>
 <!-- this button is for updateing next transaction-->
    <div class="row ">
      <button class="mybutton" type="submit" {{action 'gotoupdate'}}>Update</button>
    </div> 

</div>


